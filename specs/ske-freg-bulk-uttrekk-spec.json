{
  "type" : "Specification",
  "name" : "Collect FREG",
  "startFunctionId" : "loop",
  "id" : "SKE-FREG-UTTREKK-BATCH",
  "configure" : [ {
    "type" : "SpecificationContext",
    "variables" : {
      "ProduksjonURL" : "https://folkeregisteret.api.skatteetaten.no/folkeregisteret/offentlig-med-hjemmel",
      "fromFeedSequence" : "0",
      "jobId" : "c5d3af2f-4ac7-4506-af67-61e71c5f0b8f",
      "nextBatch" : "0"
    },
    "globalState" : {
      "global.topic" : "freg-uttrekk-komplett"
    }
  }, {
    "type" : "Security",
    "bundleName" : "ske-p12-certs"
  } ],
  "functions" : {
    "create-job" : {
      "type" : "Get",
      "id" : "create-job",
      "url" : "${ProduksjonURL}/api/v1/uttrekk/komplett?feedsekvensnr=${fromFeedSequence}",
      "requestHeaders" : { },
      "responseValidators" : [ {
        "type" : "HttpStatusValidation",
        "success" : {
          "200" : [ ]
        }
      } ],
      "pipes" : [ {
        "type" : "Execute",
        "executeId" : "loop",
        "requiredInputs" : [ "jobId" ],
        "inputVariables" : {
          "jobId" : {
            "type" : "QueryJqPath",
            "expression" : ".jobbId"
          }
        }
      } ]
    },
    "loop" : {
      "type" : "Paginate",
      "id" : "loop",
      "variables" : {
        "fromBatch" : "${nextBatch}"
      },
      "threshold" : 100000,
      "addPageContent" : false,
      "iterate" : [ {
        "type" : "Execute",
        "executeId" : "batch-list",
        "requiredInputs" : [ "jobId", "fromBatch" ]
      } ],
      "until" : {
        "type" : "ConditionWhenVariableIsNull",
        "identifier" : "nextBatch"
      }
    },
    "batch-list" : {
      "type" : "Get",
      "id" : "batch-list",
      "url" : "${ProduksjonURL}/api/v1/uttrekk/${jobId}/batch/${fromBatch}",
      "requestHeaders" : { },
      "returnVariables" : [ "nextBatch" ],
      "responseValidators" : [ {
        "type" : "HttpStatusValidation",
        "success" : {
          "200" : [ ]
        }
      } ],
      "pipes" : [ {
        "type" : "Sequence",
        "splitQuery" : {
          "type" : "QueryJqPath",
          "expression" : ".dokumentidentifikator[]"
        },
        "expectedQuery" : {
          "type" : "QueryJqPath",
          "expression" : "."
        }
      }, {
        "type" : "NextPage",
        "outputs" : {
          "nextBatch" : {
            "type" : "QueryEval",
            "expression" : "${cast.toLong(fromBatch) + 1}"
          }
        }
      }, {
        "type" : "Parallel",
        "variables" : {
          "position" : {
            "type" : "QueryJqPath",
            "expression" : "."
          }
        },
        "splitQuery" : {
          "type" : "QueryJqPath",
          "expression" : ".dokumentidentifikator[]"
        },
        "pipes" : [ {
          "type" : "AddContent",
          "positionVariableExpression" : "${position}",
          "contentKey" : "entry"
        }, {
          "type" : "Execute",
          "executeId" : "person-document",
          "inputVariables" : {
            "personDocumentId" : {
              "type" : "QueryJqPath",
              "expression" : "."
            }
          }
        }, {
          "type" : "Publish",
          "positionVariableExpression" : "${position}"
        } ]
      } ]
    },
    "person-document" : {
      "type" : "Get",
      "id" : "person-document",
      "url" : "${ProduksjonURL}/api/v1/personer/arkiv/${personDocumentId}?part=person-basis&part=identitetsgrunnlag-utvidet&part=relasjon-utvidet&part=utlendingsmyndighetenesIdentifikasjonsnummer&part=innflytting&part=utflytting&part=foedselINorge&part=opphold&part=forholdTilSametingetsValgmanntall",
      "requestHeaders" : {
        "Accept" : [ "application/xml" ]
      },
      "responseValidators" : [ {
        "type" : "HttpStatusValidation",
        "success" : {
          "200" : [ ]
        }
      } ],
      "pipes" : [ {
        "type" : "AddContent",
        "positionVariableExpression" : "${position}",
        "contentKey" : "person"
      } ]
    }
  }
}